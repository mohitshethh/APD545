<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="com.mohit.workshop2.controller.OrderSummaryController">
    <Text fx:id="txtOrderSummary" wrappingWidth="300">
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin></Text>
    <Button alignment="BOTTOM_CENTER" onAction="#closeWindow" text="Close">
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="100.0" />
      </VBox.margin></Button>
</VBox>
